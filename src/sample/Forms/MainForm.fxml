<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Forms.MainController">
    <children>
          <Button id="btnIncome" mnemonicParsing="false" onMouseClicked="#btnIncome_click" prefHeight="25.0" prefWidth="77.0" text="Доходы" />
          <Button id="btnOutCome" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#btnOutCome_click" prefHeight="25.0" prefWidth="86.0" text="Расходы" GridPane.columnIndex="2" />
          <Button id="btnAccounts" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#btnAccounts_click" prefHeight="25.0" prefWidth="86.0" text="Счета" GridPane.columnIndex="1" />
          <Button id="btnSettings" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#btnSettings_click" prefHeight="25.0" prefWidth="87.0" text="Настройки" GridPane.columnIndex="3" />
    </children>
    <columnConstraints>
        <ColumnConstraints />
      <ColumnConstraints maxWidth="105.0" minWidth="10.0" prefWidth="73.0" />
      <ColumnConstraints maxWidth="147.0" minWidth="10.0" prefWidth="78.0" />
      <ColumnConstraints maxWidth="151.0" minWidth="10.0" prefWidth="80.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="25.0" minHeight="16.0" prefHeight="25.0" />
        <RowConstraints maxHeight="9.0" minHeight="0.0" prefHeight="0.0" />
    </rowConstraints>
</GridPane>
